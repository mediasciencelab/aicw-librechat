
[Unit]
Description = chat-libre

[Service]
Type           = oneshot
User           = ubuntu
Group          = ubuntu
LimitNOFILE    = 4096
RestartSec     = 5s
StandardOutput = append:/var/log/errors.log
StandardError  = append:/var/log/errors.log
ExecStart      = /usr/bin/docker compose -f /home/ubuntu/docker-compose.mediasci.yml up -d --pull never

[Install]
WantedBy = multi-user.target
